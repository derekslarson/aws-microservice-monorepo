{
  "name": "yac-api-v4",
  "private": true,
  "workspaces": [
    "microservices/*",
    "microservices/auth/ui",
    "microservices/*/e2e/test-stack"
  ],
  "version": "0.1.0",
  "engines": {
    "node": "^14.15.0"
  },
  "scripts": {
    "build:local": "lerna run build:local",
    "build:dev": "lerna run build:dev",
    "build:stage": "lerna run build:stage",
    "build:prod": "lerna run build:prod",
    "synth:local": "lerna run synth:local",
    "synth:dev": "lerna run synth:dev",
    "synth:stage": "lerna run synth:stage",
    "synth:prod": "lerna run synth:prod",
    "clean": "lerna run clean",
    "build:unit-tests": "yarn clean:unit-tests && tsc --project ./tsconfig.test.json",
    "clean:unit-tests": "rm -rf ./.nyc_output && rm -rf ./coverage && rm -rf ./compiled_tests",
    "nyc": "nyc node config/jasmine/jasmine.runner.js",
    "test:unit": "yarn clean:unit-tests && yarn build:unit-tests && yarn nyc"
  },
  "devDependencies": {
    "@types/case-sensitive-paths-webpack-plugin": "^2.1.6",
    "@types/jasmine": "^3.6.4",
    "@types/terser-webpack-plugin": "^5.2.0",
    "@types/yargs": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "aws-cdk": "2.2.0",
    "aws-cdk-lib": "~2.2.0",
    "constructs": "^10.0.13",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-typescript": "^11.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^4.3.1",
    "jasmine": "^3.6.4",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "lerna": "^4.0.0",
    "nyc": "^15.1.0",
    "terser-webpack-plugin": "^5.3.0",
    "ts-loader": "^8.0.17",
    "ts-node": "^8.1.0",
    "typescript": "^4.2.2",
    "webpack": "^5.24.1",
    "webpack-cli": "^4.5.0",
    "yargs": "^17.2.0"
  },
  "dependencies": {},
  "resolutions": {
    "@types/jasmine": "^3.6.4"
  }
}
