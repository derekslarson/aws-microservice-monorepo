{
  "name": "@yac/api-v4",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "lerna run start --stream --sort",
    "build": "lerna run build --stream --sort",
    "test": "lerna run test --stream --parallel",
    "lint": "lerna run lint --stream --parallel",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Juan Di Toro",
  "devDependencies": {
    "@aws-cdk/assert": "^1.91.0",
    "@aws-cdk/aws-apigatewayv2": "^1.91.0",
    "@aws-cdk/aws-iam": "^1.91.0",
    "@aws-cdk/aws-lambda": "^1.91.0",
    "@aws-cdk/aws-lambda-event-sources": "^1.91.0",
    "@aws-cdk/aws-ssm": "^1.91.0",
    "@aws-cdk/core": "^1.91.0",
    "@types/aws-lambda": "^8.10.72",
    "@types/elasticsearch": "^5.0.37",
    "aws-cdk": "^1.91.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "^5.1.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5"
  }
}
