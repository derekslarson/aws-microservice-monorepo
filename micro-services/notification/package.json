{
  "name": "@yac/notification",
  "version": "0.1.0",
  "scripts": {
    "build:local": "yarn clean && webpack --bail --config ../../config/webpack/webpack.config.dev.js && yarn build:layer",
    "build:dev": "yarn clean && webpack --bail --config ../../config/webpack/webpack.config.dev.js && yarn build:layer",
    "build:stage": "yarn clean && webpack --bail --config ../../config/webpack/webpack.config.release.js && yarn build:layer",
    "build:prod": "yarn clean && webpack --bail --config ../../config/webpack/webpack.config.release.js && yarn build:layer",
    "build:layer": "ts-node ../../config/layer/buildLayer.ts",
    "build:unit-tests": "yarn clean:unit-tests && tsc --project tsconfig.test.json",
    "build:e2e-tests": "yarn clean:unit-tests && tsc --project tsconfig.e2e.test.json",
    "clean": "rm -rf ./dist",
    "clean:unit-tests": "rm -rf ./.nyc_output && rm -rf ./coverage && rm -rf ./compiled_tests",
    "clean:e2e-tests": "rm -rf ./compiled_e2e_tests",
    "synth:local": "cdk synth --context environment=local --context developer=$(whoami)",
    "synth:dev": "cdk synth --context environment=dev",
    "synth:stage": "cdk synth --context environment=stage",
    "synth:prod": "cdk synth --context environment=prod",
    "deploy:local": "yarn workspace @yac/util build:library && yarn build:local && cdk deploy --context environment=local --context developer=$(whoami)",
    "deploy:dev": "yarn workspace @yac/util build:library && yarn build:dev && cdk deploy --context environment=dev",
    "deploy:stage": "yarn workspace @yac/util build:library && yarn build:stage && cdk deploy --context environment=stage",
    "deploy:prod": "yarn workspace @yac/util build:library && yarn build:prod && cdk deploy --context environment=prod",
    "destroy:local": "cdk destroy --context environment=local --context developer=$(whoami)",
    "nyc": "nyc node ../../config/jasmine/jasmine.runner.js",
    "nyc:coverage-report": "nyc node ../../config/jasmine/jasmine.runner.js --reporter html --reporter text -t coverage --report-dir coverage/summary",
    "test:e2e": "yarn clean:e2e-tests && yarn build:e2e-tests && ts-node ./e2e/runner.ts --environment=$(whoami)",
    "test:unit": "yarn clean:unit-tests && yarn build:unit-tests && yarn nyc"
  },
  "devDependencies": {
    "@aws-cdk/assert": "~1.114.0",
    "@aws-cdk/aws-apigatewayv2": "~1.114.0",
    "@aws-cdk/aws-apigatewayv2-integrations": "~1.114.0",
    "@aws-cdk/aws-certificatemanager": "~1.114.0",
    "@aws-cdk/aws-dynamodb": "~1.114.0",
    "@aws-cdk/aws-iam": "~1.114.0",
    "@aws-cdk/aws-lambda": "~1.114.0",
    "@aws-cdk/aws-lambda-event-sources": "~1.114.0",
    "@aws-cdk/aws-route53": "~1.114.0",
    "@aws-cdk/aws-route53-targets": "~1.114.0",
    "@aws-cdk/aws-s3": "~1.114.0",
    "@aws-cdk/aws-s3-notifications": "~1.114.0",
    "@aws-cdk/aws-sns": "~1.114.0",
    "@aws-cdk/aws-ssm": "~1.114.0",
    "@aws-cdk/core": "~1.114.0",
    "@aws-cdk/custom-resources": "~1.114.0",
    "@types/aws-lambda": "^8.10.72",
    "@types/elasticsearch": "^5.0.37",
    "@types/jasmine": "^3.6.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/jwk-to-pem": "^2.0.1",
    "@types/node": "10.17.27",
    "@types/sharp": "^0.28.4",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.4.7",
    "@types/yargs": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "aws-cdk": "~1.114.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-typescript": "^11.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^4.3.1",
    "jasmine": "^3.6.4",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "ksuid": "^2.0.0",
    "nyc": "^15.1.0",
    "ts-loader": "^8.0.17",
    "ts-node": "^8.1.0",
    "typescript": "^4.2.2",
    "webpack": "^5.24.1",
    "webpack-cli": "^4.5.0",
    "ws": "^8.0.0",
    "yargs": "^17.0.1"
  },
  "dependencies": {
    "@yac/util": "0.1.0",
    "aws-sdk": "^2.783.0",
    "axios": "^0.21.1",
    "crypto-secure-random-digit": "^1.0.9",
    "inversify": "^5.0.1",
    "jdenticon": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.5",
    "reflect-metadata": "^0.1.13",
    "runtypes": "^6.3.1",
    "source-map-support": "^0.5.16"
  }
}
