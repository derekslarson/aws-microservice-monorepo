{
  "name": "@yac/auth",
  "version": "0.1.0",
  "bin": {
    "yac-auth-service": "bin/yac-auth-service.js"
  },
  "scripts": {
    "build:local": "yarn clean && yarn clean && yarn workspace @yac/auth-ui build:local && NODE_OPTIONS=--max-old-space-size=8192 webpack --bail --config ../../config/webpack/webpack.config.ts",
    "build:dev": "yarn clean && yarn clean && yarn workspace @yac/auth-ui build:dev && NODE_OPTIONS=--max-old-space-size=8192 webpack --bail --config ../../config/webpack/webpack.config.ts",
    "build:stage": "yarn clean && yarn clean && yarn workspace @yac/auth-ui build:stage && NODE_OPTIONS=--max-old-space-size=8192 webpack --bail --config ../../config/webpack/webpack.config.ts",
    "build:prod": "yarn clean && yarn clean && yarn workspace @yac/auth-ui build:prod && NODE_OPTIONS=--max-old-space-size=8192 webpack --bail --config ../../config/webpack/webpack.config.ts",
    "build:unit-tests": "yarn clean:unit-tests && tsc --project tsconfig.test.json",
    "clean": "rm -rf ./dist && rm -rf ./cdk.out",
    "clean:unit-tests": "rm -rf ./.nyc_output && rm -rf ./coverage && rm -rf ./compiled_tests",
    "synth:local": "cdk synth --context environment=local --context developer=$(whoami)",
    "synth:dev": "cdk synth --context environment=dev",
    "synth:stage": "cdk synth --context environment=stage",
    "synth:prod": "cdk synth --context environment=prod",
    "deploy:local": "yarn build:local && cdk deploy --context environment=local --context developer=$(whoami)",
    "deploy:dev": "yarn build:dev && cdk deploy --context environment=dev",
    "deploy:stage": "yarn build:stage && cdk deploy --context environment=stage",
    "deploy:prod": "yarn build:prod && cdk deploy --context environment=prod",
    "destroy:local": "cdk destroy --context environment=local --context developer=$(whoami)",
    "nyc": "nyc node ../../config/jasmine/jasmine.runner.js",
    "nyc:coverage-report": "nyc --reporter html --reporter text -t coverage --report-dir coverage/summary node ../../config/jasmine/jasmine.runner.js",
    "test:unit": "yarn run build:unit-tests && yarn run nyc",
    "test:unit:coverage-report": "yarn run build:unit-tests && yarn run nyc:coverage-report"
  },
  "devDependencies": {
    "@aws-cdk/assert": "~1.114.0",
    "@aws-cdk/aws-apigatewayv2": "~1.114.0",
    "@aws-cdk/aws-apigatewayv2-integrations": "~1.114.0",
    "@aws-cdk/aws-certificatemanager": "~1.114.0",
    "@aws-cdk/aws-cloudfront": "~1.114.0",
    "@aws-cdk/aws-cloudfront-origins": "~1.114.0",
    "@aws-cdk/aws-cognito": "~1.114.0",
    "@aws-cdk/aws-dynamodb": "~1.114.0",
    "@aws-cdk/aws-events": "~1.114.0",
    "@aws-cdk/aws-events-targets": "~1.114",
    "@aws-cdk/aws-iam": "~1.114.0",
    "@aws-cdk/aws-lambda": "~1.114.0",
    "@aws-cdk/aws-lambda-event-sources": "~1.114.0",
    "@aws-cdk/aws-route53": "~1.114.0",
    "@aws-cdk/aws-route53-targets": "~1.114.0",
    "@aws-cdk/aws-s3": "~1.114.0",
    "@aws-cdk/aws-s3-deployment": "~1.114.0",
    "@aws-cdk/aws-secretsmanager": "~1.114.0",
    "@aws-cdk/aws-sns": "~1.114.0",
    "@aws-cdk/aws-sns-subscriptions": "~1.114.0",
    "@aws-cdk/aws-sqs": "~1.114.0",
    "@aws-cdk/aws-ssm": "~1.114.0",
    "@aws-cdk/core": "~1.114.0",
    "@aws-cdk/custom-resources": "~1.114.0",
    "@types/aws-lambda": "^8.10.85",
    "@types/elasticsearch": "^5.0.37",
    "@types/jasmine": "^3.6.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "10.17.27",
    "@types/node-jose": "^1.1.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "aws-cdk": "~1.114.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-typescript": "^11.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^4.3.1",
    "jasmine": "^3.6.4",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "nyc": "^15.1.0",
    "ts-loader": "^8.0.17",
    "ts-node": "^8.1.0",
    "typescript": "^4.2.2",
    "webpack": "^5.24.1",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "@slack/web-api": "^6.5.1",
    "@yac/util": "0.1.0",
    "aws-sdk": "^2.783.0",
    "csrf": "^3.1.0",
    "inversify": "^5.0.1",
    "node-jose": "^2.0.0",
    "pkce-challenge": "^2.2.0",
    "reflect-metadata": "^0.1.13",
    "runtypes": "^6.3.1"
  }
}
