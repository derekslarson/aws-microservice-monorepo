{
  "name": "@yac/identity-service",
  "version": "0.1.0",
  "bin": {
    "yac-auth-service": "bin/yac-identity-service.js"
  },
  "scripts": {
    "build:local": "yarn clean && yarn workspace @yac/core build:local && webpack --bail --config ../../config/webpack/webpack.config.dev.js && yarn build:layer",
    "build:dev": "yarn clean && yarn workspace @yac/core build:dev && webpack --bail --config ../../config/webpack/webpack.config.dev.js && yarn build:layer",
    "build:stage": "yarn clean && yarn workspace @yac/core build:stage && webpack --bail --config ../../config/webpack/webpack.config.release.js && yarn build:layer",
    "build:prod": "yarn clean && yarn workspace @yac/core build:prod && webpack --bail --config ../../config/webpack/webpack.config.release.js && yarn build:layer",
    "build:layer": "ts-node ../../config/layer/buildLayer.ts",
    "build:unit-tests": "yarn clean:unit-tests && tsc --project tsconfig.test.json",
    "clean": "rm -rf ./dist",
    "clean:unit-tests": "rm -rf ./.nyc_output && rm -rf ./coverage && rm -rf ./compiled_tests",
    "synth:local": "cdk synth --context environment=local --context developer=$(whoami)",
    "synth:dev": "cdk synth --context environment=dev",
    "synth:stage": "cdk synth --context environment=stage",
    "synth:prod": "cdk synth --context environment=prod",
    "deploy:local": "yarn build:dev && cdk deploy --context environment=local --context developer=$(whoami)",
    "deploy:dev": "yarn build:dev && cdk deploy --context environment=dev",
    "deploy:stage": "yarn build && cdk deploy --context environment=stage",
    "deploy:prod": "yarn build && cdk deploy --context environment=prod",
    "destroy:local": "cdk destroy --context environment=local --context developer=$(whoami)",
    "nyc": "nyc node ../../config/jasmine/jasmine.runner.js",
    "test:unit": "yarn build:unit-tests && yarn nyc"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.91.0",
    "@aws-cdk/aws-apigatewayv2": "^1.91.0",
    "@aws-cdk/aws-apigatewayv2-integrations": "^1.91.0",
    "@aws-cdk/aws-cognito": "^1.91.0",
    "@aws-cdk/aws-dynamodb": "^1.91.0",
    "@aws-cdk/aws-iam": "^1.91.0",
    "@aws-cdk/aws-lambda": "^1.91.0",
    "@aws-cdk/aws-lambda-event-sources": "^1.91.0",
    "@aws-cdk/aws-ssm": "^1.91.0",
    "@aws-cdk/core": "^1.91.0",
    "@aws-cdk/custom-resources": "^1.91.0",
    "@types/aws-lambda": "^8.10.72",
    "@types/elasticsearch": "^5.0.37",
    "@types/jasmine": "^3.6.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "10.17.27",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "aws-cdk": "^1.91.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-typescript": "^11.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^4.3.1",
    "jasmine": "^3.6.4",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "nyc": "^15.1.0",
    "ts-loader": "^8.0.17",
    "ts-node": "^8.1.0",
    "typescript": "^4.2.2",
    "webpack": "^5.24.1",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "@yac/core": "0.1.0",
    "aws-sdk": "^2.783.0",
    "axios": "^0.21.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "crypto": "^1.0.1",
    "crypto-secure-random-digit": "^1.0.9",
    "inversify": "^5.0.1",
    "reflect-metadata": "^0.1.13",
    "serialize-error": "^7.0.1",
    "source-map-support": "^0.5.16",
    "uuid": "^8.3.1"
  }
}
