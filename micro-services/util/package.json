{
  "name": "@yac/util",
  "version": "0.1.0",
  "bin": {
    "yac-core": "bin/yac-util.js"
  },
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "build:library": "yarn clean:library && webpack --bail --config ./config/webpack.config.js && cp ./package.json ./lib/package.json",
    "build:layer": "ts-node ../../config/layer/buildLayer.ts",
    "build:handlers": "yarn clean:handlers && webpack --bail --config ../../config/webpack/webpack.config.release.js && yarn build:layer",
    "build": "yarn build:library && yarn build:handlers && yarn build:layer",
    "build:unit-tests": "yarn clean:unit-tests && tsc --project tsconfig.test.json",
    "clean:library": "rm -rf ./lib",
    "clean:handlers": "rm -rf ./dist",
    "clean:unit-tests": "rm -rf ./.nyc_output && rm -rf ./coverage && rm -rf ./compiled_tests",
    "synth:local": "cdk synth --context environment=local --context developer=$(whoami)",
    "synth:dev": "cdk synth --context environment=dev",
    "synth:stage": "cdk synth --context environment=stage",
    "synth:prod": "cdk synth --context environment=prod",
    "deploy:local": "yarn build:handlers && cdk deploy --context environment=local --context developer=$(whoami)",
    "deploy:dev": "yarn build:handlers && cdk deploy --context environment=dev",
    "deploy:stage": "yarn build:handlers && cdk deploy --context environment=stage",
    "deploy:prod": "yarn build:handlers && cdk deploy --context environment=prod",
    "destroy:local": "cdk destroy --context environment=local --context developer=$(whoami)",
    "nyc": "nyc node ../../config/jasmine/jasmine.runner.js",
    "nyc:coverage-report": "nyc --reporter html --reporter text -t coverage --report-dir coverage/summary node ../../config/jasmine/jasmine.runner.js",
    "test:unit": "yarn run build:unit-tests && yarn run nyc",
    "test:unit:coverage-report": "yarn run build:unit-tests && yarn run nyc:coverage-report"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigatewayv2": "~1.114.0",
    "@aws-cdk/aws-apigatewayv2-integrations": "~1.114.0",
    "@aws-cdk/aws-certificatemanager": "~1.114.0",
    "@aws-cdk/aws-cognito": "~1.114.0",
    "@aws-cdk/aws-dynamodb": "~1.114.0",
    "@aws-cdk/aws-iam": "~1.114.0",
    "@aws-cdk/aws-lambda": "~1.114.0",
    "@aws-cdk/aws-lambda-event-sources": "~1.114.0",
    "@aws-cdk/aws-route53": "~1.114.0",
    "@aws-cdk/aws-route53-targets": "~1.114.0",
    "@aws-cdk/aws-s3": "~1.114.0",
    "@aws-cdk/aws-sns": "~1.114.0",
    "@aws-cdk/aws-ssm": "~1.114.0",
    "@aws-cdk/core": "~1.114.0",
    "@aws-cdk/custom-resources": "~1.114.0",
    "@types/aws-lambda": "^8.10.72",
    "@types/elasticsearch": "^5.0.37",
    "@types/jasmine": "^3.6.4",
    "@types/node": "10.17.27",
    "@types/rmfr": "^2.0.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "aws-cdk": "~1.114.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-typescript": "^11.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^4.3.1",
    "jasmine": "^3.6.4",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "npm-dts-webpack-plugin": "^1.3.7",
    "nyc": "^15.1.0",
    "ts-loader": "^8.0.17",
    "ts-node": "^8.1.0",
    "typescript": "^4.2.3",
    "webpack": "^5.24.1",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "aws-sdk": "^2.783.0",
    "axios": "^0.21.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "crypto-secure-random-digit": "^1.0.9",
    "inversify": "^5.0.1",
    "ksuid": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "rmfr": "^2.0.0",
    "runtypes": "^6.3.1",
    "serialize-error": "^7.0.1",
    "source-map-support": "^0.5.16",
    "uuid": "^8.3.1"
  }
}
