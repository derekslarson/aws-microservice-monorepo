{
  "name": "@yac/core",
  "version": "0.1.0",
  "bin": {
    "yac-core": "bin/yac-core.js"
  },
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "yarn clean && webpack --bail --config ./config/webpack.config.js && cp ./package.json ./dist/package.json",
    "build:local": "yarn build",
    "build:dev": "yarn build",
    "build:stage": "yarn build",
    "build:prod": "yarn build",
    "build:unit-tests": "yarn run clean:unit-tests && tsc --project ../../tsconfig.test.json",
    "clean": "rm -rf ./dist",
    "clean:unit-tests": "rm -rf ./.nyc_output && rm -rf ./coverage && rm -rf ./compiled_tests",
    "synth:local": "cdk synth --context environment=local --context developer=$(whoami)",
    "synth:dev": "cdk synth --context environment=dev",
    "synth:stage": "cdk synth --context environment=stage",
    "synth:prod": "cdk synth --context environment=prod",
    "deploy:local": "yarn run build:local && cdk deploy --context environment=local --context developer=$(whoami)",
    "deploy:dev": "yarn run build:dev && cdk deploy --context environment=dev",
    "deploy:stage": "yarn run build:stage && cdk deploy --context environment=stage",
    "deploy:prod": "yarn run build:prod && cdk deploy --context environment=prod",
    "destroy:local": "cdk destroy --context environment=local --context developer=$(whoami)",
    "nyc": "nyc node ../../config/jasmine/jasmine.runner.js",
    "test:unit": "yarn run build:unit-tests && yarn run nyc"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigatewayv2": "^1.94.1",
    "@aws-cdk/aws-apigatewayv2-integrations": "^1.94.1",
    "@aws-cdk/aws-certificatemanager": "^1.94.1",
    "@aws-cdk/aws-lambda": "^1.94.1",
    "@aws-cdk/aws-route53": "^1.94.1",
    "@aws-cdk/aws-route53-targets": "^1.94.1",
    "@aws-cdk/aws-sns": "^1.94.1",
    "@aws-cdk/aws-ssm": "^1.105.0",
    "@aws-cdk/core": "^1.94.1",
    "@types/aws-lambda": "^8.10.72",
    "@types/elasticsearch": "^5.0.37",
    "@types/jasmine": "^3.6.4",
    "@types/node": "10.17.27",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "aws-cdk": "^1.94.1",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-typescript": "^11.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^4.3.1",
    "jasmine": "^3.6.4",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "npm-dts-webpack-plugin": "^1.3.7",
    "nyc": "^15.1.0",
    "ts-loader": "^8.0.17",
    "ts-node": "^8.1.0",
    "typescript": "^4.2.3",
    "webpack": "^5.24.1",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "aws-sdk": "^2.783.0",
    "axios": "^0.21.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "crypto-secure-random-digit": "^1.0.9",
    "inversify": "^5.0.1",
    "reflect-metadata": "^0.1.13",
    "serialize-error": "^7.0.1",
    "source-map-support": "^0.5.16",
    "uuid": "^8.3.1"
  }
}
