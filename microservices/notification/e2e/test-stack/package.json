{
  "name": "@yac/notification-testing",
  "version": "0.1.0",
  "scripts": {
    "build": "yarn clean && NODE_OPTIONS=--max-old-space-size=8192 webpack --config ../../../../config/webpack/webpack.config.ts",
    "build:local": "BUILD_ENV=$(whoami) yarn build",
    "build:dev": "BUILD_ENV=dev yarn build",
    "clean": "rm -rf ./dist && rm -rf ./cdk.out",
    "synth": "yarn build && cdk synth",
    "synth:local": "yarn build:local && cdk synth",
    "synth:dev": "yarn build:dev && cdk synth",
    "deploy": "yarn build && cdk deploy",
    "deploy:local": "yarn build:local && cdk deploy",
    "deploy:dev": "yarn build:dev && cdk deploy"
  },
  "devDependencies": {
    "@yac/util": "0.1.0",
    "@aws-cdk/aws-dynamodb": "~1.114.0",
    "@aws-cdk/aws-lambda": "~1.114.0",
    "@aws-cdk/aws-lambda-event-sources": "~1.114.0",
    "@aws-cdk/aws-sns": "~1.114.0",
    "@aws-cdk/aws-ssm": "~1.114.0",
    "@aws-cdk/core": "~1.114.0",
    "@types/node": "10.17.27",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "aws-cdk": "~1.114.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-typescript": "^11.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^4.3.1",
    "ts-loader": "^8.0.17",
    "ts-node": "^8.1.0",
    "typescript": "^4.2.2",
    "webpack": "^5.24.1",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "aws-sdk": "^2.783.0"
  }
}
