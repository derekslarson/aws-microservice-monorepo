{
  "name": "@yac/chunked-upload-testing",
  "version": "0.1.0",
  "scripts": {
    "build:local": "yarn clean && webpack --bail --config ../../../../config/webpack/webpack.config.ts",
    "build:dev": "yarn clean && webpack --bail --config ../../../../config/webpack/webpack.config.ts",
    "clean": "rm -rf ./dist && rm -rf ./cdk.out",
    "synth:local": "cdk synth --context environment=$(whoami)",
    "synth:dev": "cdk synth --context environment=dev",
    "deploy:local": "yarn build:local && cdk deploy --context environment=alpha",
    "deploy:dev": "yarn build:dev && cdk deploy --context environment=dev",
    "destroy:local": "cdk destroy --context environment=$(whoami)"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.85",
    "@types/node": "10.17.27",
    "@types/node-jose": "^1.1.8",
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.2.0-alpha.0",
    "aws-cdk": "2.2.0",
    "aws-cdk-lib": "~2.2.0",
    "constructs": "^10.0.13"
  },
  "dependencies": {
    "aws-sdk": "^2.783.0"
  }
}
