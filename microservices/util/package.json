{
  "name": "@yac/util",
  "version": "0.1.0",
  "scripts": {
    "build:local": "yarn clean && NODE_OPTIONS=--max-old-space-size=8192 webpack --bail --config ../../config/webpack/webpack.config.ts",
    "build:local:v2": "yarn clean && NODE_OPTIONS=--max-old-space-size=8192 webpack --bail --config ../../config/webpack/webpack.config.ts",
    "build:dev": "yarn clean && NODE_OPTIONS=--max-old-space-size=8192 webpack --bail --config ../../config/webpack/webpack.config.ts",
    "build:stage": "yarn clean && NODE_OPTIONS=--max-old-space-size=8192 webpack --env --bail --config ../../config/webpack/webpack.config.ts",
    "build:prod": "yarn clean && NODE_OPTIONS=--max-old-space-size=8192 webpack --bail --config ../../config/webpack/webpack.config.ts",
    "build:unit-tests": "yarn clean:unit-tests && tsc --project tsconfig.test.json",
    "clean": "rm -rf ./dist",
    "clean:unit-tests": "rm -rf ./.nyc_output && rm -rf ./coverage && rm -rf ./compiled_tests",
    "synth:local": "yarn build:local && cdk synth --context environment=local --context developer=$(whoami)",
    "synth:dev": "yarn build:dev && cdk synth --context environment=dev",
    "synth:stage": "yarn build:stage && cdk synth --context environment=stage",
    "synth:prod": "yarn build:prod && cdk synth --context environment=prod",
    "deploy:local": "yarn build:local && cdk deploy --context environment=local --context developer=$(whoami)",
    "deploy:dev": "yarn build:dev && cdk deploy --context environment=dev",
    "deploy:stage": "yarn build:stage && cdk deploy --context environment=stage",
    "deploy:prod": "yarn build:prod && cdk deploy --context environment=prod",
    "destroy:local": "cdk destroy --context environment=local --context developer=$(whoami)",
    "nyc": "nyc node ../../config/jasmine/jasmine.runner.js",
    "nyc:coverage-report": "nyc --reporter html --reporter text -t coverage --report-dir coverage/summary node ../../config/jasmine/jasmine.runner.js",
    "test:unit": "yarn run build:unit-tests && yarn run nyc",
    "test:unit:coverage-report": "yarn run build:unit-tests && yarn run nyc:coverage-report"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.2.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-authorizers-alpha": "^2.2.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.2.0-alpha.0",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/rmfr": "^2.0.1",
    "aws-cdk": "2.2.0",
    "aws-cdk-lib": "~2.2.0",
    "constructs": "^10.0.13"
  },
  "dependencies": {
    "aws-sdk": "^2.783.0",
    "axios": "^0.21.1",
    "crypto-secure-random-digit": "^1.0.9",
    "google-auth-library": "^7.11.0",
    "inversify": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "ksuid": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "rmfr": "^2.0.0",
    "runtypes": "^6.3.1",
    "serialize-error": "^7.0.1"
  }
}
