{
  "cache": false,
  "reporter": ["text", "lcov"],
  "check-coverage": true,
  "lines": 80,
  "functions": 80,
  "branches": 80,
  "extension": [
    ".ts"
  ],
  "exclude": [
    "**/config/**",
    "**/coverage/**",
    "**/dist/**",
    "**/lib/**",
    "**/infra/**",
    "**/src/config/**",
    "**/src/enums/**",
    "**/src/models/**",
    "**/src/types/**",
    "**/src/inversion-of-control/**",
    "**/src/factories",
    "**/src/test-support/**",
    "**/src/**/*.spec.ts",
    "**/*error.ts"
  ]
}